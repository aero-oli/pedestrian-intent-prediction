{
    "name": "Graph_Net",
    "n_gpu": 1,
    "arch":
    {
        "type": "GraphModel",
        "args": {}
    },
    "data_loader":
    {
        "type": "GraphDataLoader",
        "args":
        {
            "datasetDir": "C:\\Users\\moose\\anaconda3\\envs\\master-thesis",
            "bufferFrames": 0,
            "batchSize": 128,
            "shuffle": true,
            "trainingSplit": 0.7,
            "numberOfWorkers": 2,
            "isTrain": true,
            "modulefocus": true
        }
    },
    "optimizer":
    {
        "type": "Adam",
        "args":
        {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "negative_likelihood_loss",
    "metrics":
    [
        "accuracy", "top_k_accuracy"
    ],
    "lr_scheduler":
    {
        "type": "StepLR",
        "args":
        {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer":
    {
        "epochs": 1,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}
